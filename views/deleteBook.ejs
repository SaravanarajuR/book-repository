<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>editBooks</title>
</head>
<body>
    <%- include("header") %>
     
<div class="row">
    <% materialList.forEach(function(mat){  %>
    
    <div style="padding: 1%; width: 250px; margin-left: 23px; border: 1px solid; background-color: antiquewhite;" class="card col-md-4 col-lg-3 col-sm-12">
    <img style="width: 200px; height: 150px;" class="card-img-top" src= <%='/uploads/' + mat.image %> alt="image"> 
    <div class="card-body">
        <center>
     <h1  class="card-title"> <%= mat.name %> </h1>
     <a href=<%= '/uploads/' + mat.pdf %> > <button class="btn btn-dark">Open</button></a>
     <a href="/deleteBook/<%= mat._id %>"><button class="btn btn-danger">Delete</button></a>
    </div>
</div>

    <% }) %>
</div>

    
    <script src="index.js"></script>
</body>
</html>